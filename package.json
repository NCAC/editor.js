{
  "name": "@editorjs/editorjs",
  "version": "2.19.1",
  "description": "Editor.js â€” Native JS, based on API and Open Source",
  "main": "dist/editor.js",
  "types": "./types/index.d.ts",
  "keywords": [
    "codex editor",
    "text editor",
    "editor",
    "editor.js",
    "editorjs"
  ],
  "scripts": {
    "build": "yarn svg && yarn bundle_js",
    "bundle_js": "node ./build/rollup.js",
    "build:win": "rimraf dist && yarn svg:win && yarn build:prod",
    "build:dev": "webpack --mode development --progress --display-error-details --display-entrypoints --watch",
    "build:prod": "webpack --mode production",
    "lint": "eslint src/ --ext .ts",
    "lint:errors": "eslint src/ --ext .ts --quiet",
    "lint:fix": "eslint src/ --ext .ts --fix",
    "svg:win": "if not exist dist md dist && yarn svg",
    "svg": "svg-sprite-generate -d src/assets/ -o dist/sprite.svg",
    "pull_tools": "git submodule update --init --recursive",
    "checkout_tools": "git submodule foreach git pull origin master"
  },
  "author": "CodeX",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codex-team/editor.js.git"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/polyfill": "^7.8.7",
    "@babel/preset-env": "^7.9.5",
    "@babel/register": "^7.9.0",
    "@babel/runtime": "^7.9.2",
    "@codexteam/shortcuts": "^1.1.1",
    "babel-plugin-add-module-exports": "^1.0.0",
    "babel-plugin-class-display-name": "^2.1.0",
    "cssnano": "^4.1.10",
    "eslint": "^6.8.0",
    "html-janitor": "^2.0.4",
    "postcss-preset-env": "^6.6.0",
    "rimraf": "^3.0.2",
    "stylelint": "^13.3.3",
    "svg-sprite-generator": "^0.0.7",
    "tslint": "^6.1.1"
  },
  "collective": {
    "type": "opencollective",
    "url": "https://opencollective.com/editorjs"
  },
  "dependencies": {
    "@babel/preset-typescript": "^7.12.1",
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-commonjs": "^16.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@rollup/plugin-typescript": "^6.1.0",
    "codex-notifier": "^1.1.2",
    "codex-tooltip": "^1.0.1",
    "generate-source-map": "^0.0.5",
    "rollup": "^2.33.2",
    "rollup-plugin-analyzer": "^3.3.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-require-context": "^1.0.1",
    "rollup-plugin-styles": "^3.11.0",
    "rollup-plugin-svg-import": "^1.5.1",
    "rollup-plugin-typescript2": "^0.29.0",
    "tslib": "^2.0.3",
    "typescript": "^4.1.1-rc"
  }
}
